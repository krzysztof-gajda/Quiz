<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.quiz">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CreateQuizActivity"></activity>
        <activity android:name=".MyQuizActivity" />
        <activity android:name=".CreateQuizMenuActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".MenuActivity" />
        <activity android:name=".WaitingActivity" />
        <activity android:name=".InvitationActivity" />
        <activity android:name=".NewGameActivity" />
        <activity android:name=".MultiplayerActivity" />
        <activity android:name=".QuizActivity" />
        <activity android:name=".SingleplayerActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NewQuestionActivity" />
        <activity android:name=".SummaryActivity" />
    </application>

</manifest>